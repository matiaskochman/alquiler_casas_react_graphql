{
  "private": true,
  "scripts": {
    "build:server": "lerna run build --scope={@airbnb/common,@airbnb/server}",
    "build:web": "lerna run build --scope={@airbnb/common,@airbnb/web}"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/rimraf",
      "**/rimraf/**",
      
      "**/react-native-elements",
      "**/react-native-elements/**",

      "**/react-native",
      "**/react-native/**",
      "**/expo",
      "**/expo/**",
      "**/react-native-typescript-transformer",
      "**/react-native-typescript-transformer/**",
      "**/metro-bundler-config-yarn-workspaces",
      "**/metro-bundler-config-yarn-workspaces/**",
      "**/react-native-vector-icons",
      "**/react-native-vector-icons/**",
      "**/prop-types",
      "**/prop-types/**",
      "**/react-router-native",
      "**/react-router-native/**",
      "**/apollo-cache-inmemory",
      "**/apollo-cache-inmemory/**",
      "**/@apollo/react-hooks",
      "**/@apollo/react-hooks/**",
      "**/apollo-link-http",
      "**/apollo-link-http/**"
    ]
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    
    "graphql": "^15.0.0",
    "graphql-code-generator": "^0.16.1",
    "graphql-tag": "^2.10.3",
    "react-apollo": "^3.1.5",
    "react-native-elements": "^1.2.7",
    "react-router-native": "^5.1.2"
  },
  "name": "airbnb",
  "devDependencies": {
    "@types/react-native-elements": "^0.18.0",
    "@types/react-router": "^5.1.5",
    "lerna": "^3.20.2"
  }
}
